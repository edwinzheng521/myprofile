<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.OrderProcessedController">
   <children>
      <ImageView fitHeight="421.0" fitWidth="713.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../Resources/Background2.PNG" />
         </image>
      </ImageView>
      <Label layoutX="163.0" layoutY="77.0" prefHeight="50.0" prefWidth="439.0" text="Your Order Has Been Processed">
         <font>
            <Font name="Book Antiqua" size="27.0" />
         </font>
      </Label>
      <Button fx:id="printReceiptButton" layoutX="273.0" layoutY="145.0" mnemonicParsing="false" onAction="#printReceiptButton" text="Print Receipt">
         <font>
            <Font name="Book Antiqua" size="18.0" />
         </font>
      </Button>
      <Button fx:id="createNewButton" layoutX="30.0" layoutY="344.0" mnemonicParsing="false" onAction="#handleCreateButton" text="Create A New One">
         <font>
            <Font size="15.0" />
         </font></Button>
      <Button fx:id="homepageButton" layoutX="587.0" layoutY="344.0" mnemonicParsing="false" onAction="#handleHomeButton" text="HomePage">
         <font>
            <Font size="15.0" />
         </font></Button>
   </children>
</AnchorPane>
