<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.ItemCartPageController">
   <children>
      <ImageView fitHeight="445.0" fitWidth="714.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../Resources/Background2.PNG" />
         </image>
      </ImageView>
      <Button fx:id="homeButton" layoutX="640.0" layoutY="1.0" mnemonicParsing="false" onAction="#handleHomeButton" prefHeight="53.0" prefWidth="61.0" style="-fx-background-image: url('Resources/home.PNG');">
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor></Button>
      <Button fx:id="continueButton" layoutX="600.0" layoutY="339.0" mnemonicParsing="false" onAction="#handlecontinueButton" text="Continue">
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
         <font>
            <Font size="15.0" />
         </font></Button>
      <Button fx:id="backButton" mnemonicParsing="false" onAction="#handleBackButton" prefHeight="53.0" prefWidth="68.0" style="-fx-background-image: url('Resources/back.PNG');">
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor></Button>
      <Label layoutX="302.0" layoutY="8.0" prefHeight="38.0" prefWidth="108.0" text="Checkout">
         <font>
            <Font size="22.0" />
         </font></Label>
      <ImageView fitHeight="38.0" fitWidth="49.0" layoutX="14.0" layoutY="66.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../Resources/cart.PNG" />
         </image>
      </ImageView>
      <TextArea fx:id="taInformation" layoutX="26.0" layoutY="104.0" prefHeight="270.0" prefWidth="284.0" />
      <Label layoutX="47.0" layoutY="72.0" text=" Items in cart">
         <font>
            <Font name="Book Antiqua" size="19.0" />
         </font>
      </Label>
      <TextField layoutX="353.0" layoutY="108.0" nodeOrientation="LEFT_TO_RIGHT" prefHeight="21.0" prefWidth="326.0">
         <cursor>
            <Cursor fx:constant="TEXT" />
         </cursor></TextField>
      <Label layoutX="357.0" layoutY="75.0" text="Notes for this order">
         <font>
            <Font name="Book Antiqua" size="16.0" />
         </font>
      </Label>
      <Button fx:id="cancelButton" layoutX="356.0" layoutY="339.0" mnemonicParsing="false" onAction="#handlecancelButton" prefHeight="31.0" prefWidth="108.0" text="Cancel Order">
         <font>
            <Font size="15.0" />
         </font>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor></Button>
      <Label fx:id="subtotalLabel" layoutX="487.0" layoutY="175.0" prefHeight="21.0" prefWidth="109.0" text="Label">
         <font>
            <Font size="14.0" />
         </font></Label>
      <Label layoutX="357.0" layoutY="175.0" prefHeight="21.0" prefWidth="139.0" text="Subtotal:              $">
         <font>
            <Font name="Bodoni MT" size="16.0" />
         </font></Label>
      <Label layoutX="357.0" layoutY="205.0" prefHeight="21.0" prefWidth="139.0" text="Estimated Tax:   $">
         <font>
            <Font name="Bodoni MT" size="16.0" />
         </font></Label>
      <Label fx:id="estimatedTaxLabel" layoutX="485.0" layoutY="207.0" text="Label" />
      <Label layoutX="354.0" layoutY="240.0" text="Delivery Fees:      $">
         <font>
            <Font name="Bodoni MT" size="16.0" />
         </font></Label>
      <Label fx:id="deliveryLabel" layoutX="485.0" layoutY="242.0" text="Label" />
      <Label layoutX="362.0" layoutY="292.0" prefHeight="26.0" prefWidth="120.0" text="Total:            $">
         <font>
            <Font name="Bernard MT Condensed" size="20.0" />
         </font></Label>
      <Label fx:id="totalLabel" layoutX="487.0" layoutY="293.0" text="Label">
         <font>
            <Font name="Bodoni MT Bold" size="20.0" />
         </font></Label>
   </children>
</AnchorPane>
